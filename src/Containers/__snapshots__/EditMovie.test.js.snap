exports[`<EditMovieForm/> Renders input component for editing of the movie 1`] = `
<div
  className="formContainer"
>
  <div
    className="inputContainer"
  >
    Title
    <input
      className="titleForm"
      defaultValue="title"
      placeholder="enter the title of the movie"
      type="text"
    />
  </div>
  <div
    className="inputContainer"
  >
    Poster
    <input
      className="titleForm"
      defaultValue="http://www.theamazingpics.com/wp-content/uploads/2014/05/Amazing-Picture-of-A-Japanese-Garden-in-Portland-USA.jpg"
      placeholder="enter the Url of the poster"
      type="text"
    />
  </div>
  <div
    className="inputContainer"
  >
    Genres
    <input
      className="genresForm"
      defaultValue="horror"
      placeholder="enter the genres of the movie"
      type="text"
    />
  </div>
  <div
    className="inputContainer"
  >
    Actors
    <input
      className="actorsForm"
      defaultValue="Vasya"
      placeholder="enter the actors of the movie"
      type="text"
    />
  </div>
  <div
    className="inputContainer"
  >
    Director
    <input
      className="directorForm"
      defaultValue="Petya"
      placeholder="enter the director of the movie"
      type="text"
    />
  </div>
  <div
    className="inputContainer"
  >
    Description
    <textarea
      className="descriptionForm"
      cols="30"
      defaultValue="This is a very good movie!"
      name="text"
      placeholder="enter the description of the movie"
      rows="6"
    />
  </div>
  <button
    id="submitCreateForm"
    onClick={[Function]}
    type="submit"
